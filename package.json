    "@storybook/addon-storyshots": "5.0.5",
    "@storybook/addons": "5.0.5",
    "@storybook/react": "5.0.5",
    "babel-loader": "^8.0.5",
    "babel-preset-react": "6.24.1",
    "eslint": "5.6.0",
